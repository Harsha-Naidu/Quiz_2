
        <% @ideas.each do |idea|%>    
            <div class="card mt-4">
            <div class="card-body">
                <h5 class="card-title"><%= link_to(idea.title, idea_path(idea.id) )%></h5>
                <p class="card-text"> <%= idea.description %></p>
            </div>
            </div>
        <% end %>

